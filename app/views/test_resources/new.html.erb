<h1>TestResources 新規作成</h1>

<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>ID</th><th>サムネイル</th><th>識別子</th><th>タイプ</th><th></th>
    </tr>
  </thead>
  <tbody>
    <%- @images.each do |image| -%>
      <tr>
        <td><%= image.id %></td>
        <td><%= image_tag image.content.url(:thumb) %></td>
        <td><%= image.name %></td>
        <td><%= image.image_type %></td>
        <td><%= link_to '選択', test_resources_url(image_id: image.id, swf_series_id: @swf_series.id), method: :post, class: 'btn btn-primary' %></td>
      </tr>
    <%- end -%>
  </tbody>
</table>
