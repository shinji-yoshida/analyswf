<h1>swf title 一覧</h1>

<%= link_to '新規作成', new_swf_title_url, class: 'btn btn-primary' %><br>
<br>

<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr><td>ID</td><td>名前</td><td>シリーズ最新バージョン</td><td></td></tr>
  </thead>
  <tbody>
    <% @swf_titles.each do |title| %>
      <tr>
        <td><%= title.id %></td>
        <td><%= title.name %></td>
        <td><%= swf_titles_latest_swf_series_version(title) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

