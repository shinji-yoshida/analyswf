<h1>swf_series 一覧</h1>

<%= link_to '新規作成', new_swf_series_path, class: 'btn btn-primary' %><br>
<br>

<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr><td>ID</td><td>名前</td><td>最新バージョン</td></tr>
  </thead>
  <tbody>
    <% @some_series.each do |series| %>
      <tr>
        <td><%= series.id %></td>
        <td><%= swf_series_name_link series %></td>
        <td><%= series.version %></td>
      </tr>
    <% end %>
  </tbody>
</table>
