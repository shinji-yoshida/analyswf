<h1><%= @swf_title.name %> シリーズ一覧</h1>

<%= link_to '新規作成', new_swf_series_url(swf_title_id: @swf_title.id), class: 'btn btn-primary' %><br>
<br>

<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr><th>ID</th><th>シリーズバージョン</th><th></th></tr>
  </thead>
  <tbody>
    <% @some_series.each do |series| %>
      <tr>
        <td><%= series.id %></td>
        <td><%= series.version %></td>
        <td><%= link_to '詳細', swf_series_url(series) %> <%= link_to 'swf 一覧', swfs_url(swf_series_id: series.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'swf タイトル一覧', swf_titles_url, class: 'btn btn-info' %>